@model ShippingDetailsViewModel

    <form asp-controller="Cart" asp-action="Complete" method="post">
        <div class="form-group">
            <label asp-for="ShippingDetail.FirstName">Ad</label>
            <input asp-for="ShippingDetail.FirstName" class="form-control" placeholder="Musteri Adi" />
            <span asp-validation-for="ShippingDetail.FirstName"></span>
        </div>
        <div class="form-group">
            <label asp-for="ShippingDetail.LastName">Soyad</label>
            <input asp-for="ShippingDetail.LastName" class="form-control" placeholder="Musteri Soyadi" />
            <span asp-validation-for="ShippingDetail.LastName"></span>
        </div>
        <div class="form-group">
            <label asp-for="ShippingDetail.Email">E-posta</label>
            <input asp-for="ShippingDetail.Email" class="form-control" placeholder="E-posta" />
            <span asp-validation-for="ShippingDetail.Email"></span>
        </div>
        <div class="form-group">
            <label asp-for="ShippingDetail.Age">Yas</label>
            <input asp-for="ShippingDetail.Age" class="form-control" placeholder="Yas" />
            <span asp-validation-for="ShippingDetail.Age"></span>
        </div>
        <div class="form-group">
            <label asp-for="ShippingDetail.City">Sehir</label>
            <input asp-for="ShippingDetail.City" class="form-control" placeholder="Sehir" />
            <span asp-validation-for="ShippingDetail.City"></span>
        </div>
        <div class="form-group">
            <label asp-for="ShippingDetail.Address">Adress</label>
            <input asp-for="ShippingDetail.Address" class="form-control" placeholder="Adress" />
            <span asp-validation-for="ShippingDetail.Address"></span>
        </div>
        <input type="submit" name="name" value="Kaydet" class="btn btn-success btn-group-sm"  />
    </form>
